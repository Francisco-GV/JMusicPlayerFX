<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" id="artist_element" alignment="TOP_CENTER" spacing="10.0" stylesheets="@../css/artists_pane_style.css"
      xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
   </padding>
   <BorderPane fx:id="picPane" id="pic_pane">
      <center>
         <Label id="lbl_pic" fx:id="lblPicture"/>
      </center>
   </BorderPane>
   <Label id="lbl_name" fx:id="lblName" alignment="TOP_CENTER" contentDisplay="TEXT_ONLY" prefWidth="145.0" text="Name"
          wrapText="true"/>
</VBox>